{
  "port": "3000",
  "timeout": 300,
  "cors": true,
  "serveStatic": {
    "enabled": true,
    "endpoints": [
      {
        "endpoint": "/app",
        "dirPath": "./ui"
      }
    ]
  },
  "serveApi": {
    "enabled": true,
    "endpoints": [
      {
        "name": "api",
        "endpoint": "/api",
        "filePath": "data-seed/json-api.hbs",
        "options": {}
      }
    ]
  },
  "serveWebsocket": {
    "enabled": true,
    "endpoints": [
      {
        "endpoint": "/ws",
        "filePath": "data-seed/json-ws.hbs",
        "type": "timer,fileWatcher",
        "interval": "1000"
      }
    ],
    "options": {}
  }
}